{
  "name": "@viazenetti/graphql-faker",
  "version": "1.1.1",
  "description": "Mock or extend your GraphQL API with faked data. No coding required. Fork of APIs.guru graphql-faker, adding fakeFunc directive.",
  "main": "dist/index.js",
  "bin": {
    "graphql-faker": "dist/index.js"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "scripts": {
    "test": "npm run check && npm run prettier:check",
    "check": "tsc --noEmit",
    "start": "nodemon src/index.ts",
    "debug": "ts-node --inspect --compilerOptions '{\"inlineSources\":true}' src/index.ts",
    "start:editor": "webpack-dev-server --config webpack.config.js",
    "build:editor": "webpack -p --config webpack.config.js",
    "build:typescript": "tsc",
    "copy:graphql": "cp src/*.graphql dist/",
    "copy:editor": "mkdir \"dist/editor\" && cp src/editor/*.html dist/editor && cp src/editor/*.js dist/editor && cp src/editor/*.css dist/editor && cp src/editor/*.svg dist/editor",
    "build:all": "rm -rf dist && mkdir dist && npm run build:editor && npm run build:typescript && npm run copy:graphql && npm run copy:editor",
    "prettier": "prettier --ignore-path .gitignore --write --list-different .",
    "prettier:check": "prettier --ignore-path .gitignore --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vz-tl/graphql-faker.git"
  },
  "author": "APIs.guru <founders@apis.guru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vz-tl/graphql-faker/issues"
  },
  "homepage": "https://github.com/vz-tl/graphql-faker#readme",
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/classnames": "2.2.10",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.9",
    "@types/faker": "5.5.1",
    "@types/react": "16.14.2",
    "@types/react-dom": "16.9.10",
    "@types/yargs": "15.0.5",
    "classnames": "2.3.1",
    "codemirror": "5.60.0",
    "codemirror-graphql": "0.13.0",
    "css-loader": "3.5.3",
    "graphiql": "1.2.0",
    "marked": "1.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "faker": "5.5.3",
    "graphql": "15.4.0",
    "graphql-voyager": "1.0.0-rc.31",
    "moment": "2.29.1",
    "node-fetch": "2.6.1",
    "open": "8.0.5",
    "yargs": "16.2.0"
  }
}
